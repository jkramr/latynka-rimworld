  <!-- EN: collapse dodged -->
  <CollapseDodged.label>обвалення стелі</CollapseDodged.label>
  <!-- EN:
    <li>tale_noun->[PAWN_nameDef] dodging a cave-in</li>
    <li>tale_noun->[PAWN_nameDef] nearly crushed by falling objects</li>
    <li>image->[PAWN_nameFull] leaping away from a collapse of debris [circumstance_group]</li>
